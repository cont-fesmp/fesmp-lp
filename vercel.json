{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/home/?$",
      "status": 301,
      "headers": {
        "Location": "/"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
